{% extends "ProjetWebClassiqueBundle::layout.html.twig" %}

{% block title %}
    Liste Chefs
{% endblock %}

{% block classiqueweb_body %}

    <h2>Liste des Chefs D'orchestre</h2>

    <p>Sélection {{ contexte }} :
        {% if initial is defined %}
            {{ initial }}
        {% elseif contexte == "par année de naissance" %}
            entre {{ naissance }} et {{ fin }}
        {% endif %}

    </p>
    <ul>
    {% for musicien in liste %}
        <li>
        <a href="{{ path('projet_web_classique_cheforchestrepage',{'id':musicien.codeMusicien}) }}">{{ musicien.nomMusicien }} {{ musicien.prénomMusicien }}</a>
    </li>
    {% else %}
    Pas de chefs pour ces paramètres là.
    {% endfor %}
    </ul>
    {% endblock %}